{
    "manifest_version": 2,
    "name": "Open With Scribe",
    "description": "redirect meduim articles to scribe.rip web app",
    "version": "1.0",
    "browser_specific_settings": {
        "gecko": {
            "strict_min_version": "scriber.1.0.0"
        }
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "48": "./images/logo.png"
    },
    "browser_action": {
        "default_icon": "./images/logo.png",
        "default_title": "Open With Scribe",
        "default_popup": "popup.html"
    }
}